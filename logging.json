{
    "version": 1,
    "filters": {
        "myfilter": {
            "()": "tiny_log.ConsoleFilter",
            "param": 1
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "filters": ["myfilter"]
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": ["console"]
    }
}
